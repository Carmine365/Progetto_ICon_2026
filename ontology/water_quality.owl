<owl:NamedIndividual rdf:about="#ph">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indica quanto l'acqua è acida o basica. Range sicuro WHO: 6.5 - 8.5.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Hardness">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Causata da sali di calcio e magnesio.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Solids">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Totale solidi disciolti (minerali, sali). Limite: 1000 mg/l.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Chloramines">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disinfettanti usati nei sistemi idrici. Limite: 4 mg/l.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Sulfate">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sostanza naturale. Alte concentrazioni alterano il gusto. Limite: 250 mg/l.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Conductivity">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Conducibilità elettrica. Limite: 800 uS/cm.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Organic_carbon">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Quantità totale di carbonio organico. Limite: 10 ppm.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Trihalomethanes">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sottoprodotti della clorazione. Cancerogeni sopra 80 ug/l.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Turbidity">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nuvolosità dell'acqua. WHO raccomanda &lt; 5.0 NTU.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#PotabilityStatus">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indica se l'acqua è sicura (1) o no (0).</descrizione_parametro>
</owl:NamedIndividual>


</rdf:RDF>
