<owl:Class rdf:about="#UnsafeWater">
  <rdfs:subClassOf rdf:resource="#WaterSample"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#AcidicWater"/>
        <rdf:Description rdf:about="#BasicWater"/>
        <rdf:Description rdf:about="#HighSulfateWater"/>
        <rdf:Description rdf:about="#TurbidWater"/>
        <rdf:Description rdf:about="#HighSolidsWater"/>
        <rdf:Description rdf:about="#HighChloraminesWater"/>
        <rdf:Description rdf:about="#HighCarbonWater"/>
        <rdf:Description rdf:about="#HighTHMWater"/>
      </owl:unionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#CorrosiveWater">
  <rdfs:subClassOf rdf:resource="#WaterSample"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#WaterSample"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_ph_value"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">6.0</xsd:maxExclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_sulfate_value"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">200.0</xsd:minExclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
