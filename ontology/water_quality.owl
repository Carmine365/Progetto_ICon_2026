<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://test.org/water_quality.owl"
         xmlns="http://test.org/water_quality.owl#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#">

<owl:Ontology rdf:about="http://test.org/water_quality.owl"/>

<owl:DatatypeProperty rdf:about="#descrizione_parametro">
  <rdfs:domain rdf:resource="#Parameter"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#has_ph_value">
  <rdfs:domain rdf:resource="#WaterSample"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#has_sulfate_value">
  <rdfs:domain rdf:resource="#WaterSample"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#has_turbidity_value">
  <rdfs:domain rdf:resource="#WaterSample"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#WaterSample">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Parameter">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#CorrosiveWater">
  <rdfs:subClassOf rdf:resource="#WaterSample"/>
</owl:Class>

<owl:Class rdf:about="#AcidicWater">
  <rdfs:subClassOf rdf:resource="#WaterSample"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#WaterSample"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_ph_value"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">6.5</xsd:maxExclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#HighSulfateWater">
  <rdfs:subClassOf rdf:resource="#WaterSample"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#WaterSample"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_sulfate_value"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">250.0</xsd:minExclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#TurbidWater">
  <rdfs:subClassOf rdf:resource="#WaterSample"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#WaterSample"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_turbidity_value"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5.0</xsd:minExclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#UnsafeWater">
  <rdfs:subClassOf rdf:resource="#WaterSample"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#AcidicWater"/>
        <rdf:Description rdf:about="#HighSulfateWater"/>
        <rdf:Description rdf:about="#TurbidWater"/>
      </owl:unionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:NamedIndividual rdf:about="#ph">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indica quanto l'acqua è acida o basica. Range sicuro WHO: 6.5 - 8.5.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Hardness">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Causata da sali di calcio e magnesio.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Solids">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Totale solidi disciolti (minerali, sali). Limite: 1000 mg/l.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Chloramines">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disinfettanti usati nei sistemi idrici.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Sulfate">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sostanza naturale. Alte concentrazioni alterano il gusto.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Conductivity">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Conducibilità elettrica legata ai minerali disciolti.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Organic_carbon">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Quantità totale di carbonio organico.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Trihalomethanes">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sottoprodotti della clorazione.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Turbidity">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nuvolosità dell'acqua. WHO raccomanda &lt; 5.0 NTU.</descrizione_parametro>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#PotabilityStatus">
  <rdf:type rdf:resource="#Parameter"/>
  <descrizione_parametro rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indica se l'acqua è sicura (1) o no (0).</descrizione_parametro>
</owl:NamedIndividual>

<swrl:Variable rdf:about="urn:swrl#w"/>

<swrl:Variable rdf:about="urn:swrl#p"/>

<swrl:Variable rdf:about="urn:swrl#s"/>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#WaterSample"/>
          <swrl:argument1 rdf:resource="urn:swrl#w"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#has_ph_value"/>
              <swrl:argument1 rdf:resource="urn:swrl#w"/>
              <swrl:argument2 rdf:resource="urn:swrl#p"/>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:BuiltinAtom>
                  <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#lessThan"/>
                  <swrl:arguments>
                    <rdf:Description>
                      <rdf:first rdf:resource="urn:swrl#p"/>
                      <rdf:rest>
                        <rdf:Description>
                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">6.0</rdf:first>
                        </rdf:Description>
                      </rdf:rest>
                    </rdf:Description>
                  </swrl:arguments>
                </swrl:BuiltinAtom>
              </rdf:first>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first>
                    <swrl:DatavaluedPropertyAtom>
                      <swrl:propertyPredicate rdf:resource="#has_sulfate_value"/>
                      <swrl:argument1 rdf:resource="urn:swrl#w"/>
                      <swrl:argument2 rdf:resource="urn:swrl#s"/>
                    </swrl:DatavaluedPropertyAtom>
                  </rdf:first>
                  <rdf:rest>
                    <rdf:Description>
                      <rdf:first>
                        <swrl:BuiltinAtom>
                          <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThan"/>
                          <swrl:arguments>
                            <rdf:Description>
                              <rdf:first rdf:resource="urn:swrl#s"/>
                              <rdf:rest>
                                <rdf:Description>
                                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">200.0</rdf:first>
                                </rdf:Description>
                              </rdf:rest>
                            </rdf:Description>
                          </swrl:arguments>
                        </swrl:BuiltinAtom>
                      </rdf:first>
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:Description>
                  </rdf:rest>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#CorrosiveWater"/>
          <swrl:argument1 rdf:resource="urn:swrl#w"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>


</rdf:RDF>
